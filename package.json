{
  "name": "tasksbook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "watch": "vite --port 3000",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "watch:nodemon": "nodemon --quiet --watch . -e cjs,mjs,js,json,ts,tsx,html --exec",
    "fix:eslint": "yarn eslint . --fix --cache",
    "fix:prettier": "yarn prettier . --write --cache",
    "watch:eslint": "yarn watch:nodemon 'yarn fix:eslint'",
    "watch:prettier": "yarn watch:nodemon 'yarn fix:prettier'"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^5.16.9",
    "@mui/material": "^5.16.9",
    "axios": "^1.7.9",
    "effector": "^23.2.3",
    "effector-forms": "^1.3.4",
    "effector-react": "^23.2.1",
    "patronum": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^7.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-helmet": "^6",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-sort": "^4.0.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.1",
    "vitest": "^2.1.8"
  },
  "packageManager": "yarn@4.5.3"
}
